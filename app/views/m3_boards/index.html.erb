<h1>Listing m3_boards</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Data</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @m3_boards.each do |m3_board| %>
      <tr>
        <td><%= m3_board.name %></td>
        <td><%= m3_board.data %></td>
        <td><%= link_to 'Show', m3_board %></td>
        <td><%= link_to 'Edit', edit_m3_board_path(m3_board) %></td>
        <td><%= link_to 'Destroy', m3_board, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New M3 board', new_m3_board_path %>
